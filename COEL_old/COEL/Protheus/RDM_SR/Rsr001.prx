// Substituido pelo assistente de conversao do AP6 IDE em 16/09/02 ==> Function FHistor
Static Function FHistor()

If li > 55
	cDetalhe:=""
	Impr(cDetalhe,"P")
EndIf

lAtiv:= .T.
cSavAlias:= Alias()
nSavReg  := RecNo()

cDetalhe:= Space(10)+OemtoAnsi("HISTORICO PROFISSIONAL ")
Impr(cDetalhe,"C")
cDetalhe:= ""
Impr(cDetalhe,"C")

dbSelectArea("SQL")
dbSetOrder(1)
dbSeek(xFilial()+SQG->QG_CURRIC)
While !Eof() .And. xFilial("SQG")+SQG->QG_CURRIC == QL_FILIAL+QL_CURRIC

	cDetalhe:=Space(15)+OemtoAnsi("Admissao Demissao Area                                Empresa                        Funcao")
	//									    99/99/99 99/99/99  001 012345678901234567890123456789 012345678901234567890123456789 01234567890123456780 	
	Impr(cDetalhe,"C")	
	
	dbSelectArea("SX5")
	dbSetOrder(1)
	dbSeek(xFilial()+"R1"+SQL->QL_AREA)

	cDetalhe:=Space(15)+DtoC(SQL->QL_DTADMIS)+" "+DtoC(SQL->QL_DTDEMIS)+"  "+SQL->QL_AREA+"-"+UPPER(Substr(SX5->X5_DESCRI,1,30))+" "+SQL->QL_EMPRESA+" "+SQL->QL_FUNCAO
	Impr(cDetalhe,"C")		
	
	cDetalhe:= Space(17)+OemtoAnsi("Atividades:")
	Impr(cDetalhe,"C")
	
	l1Vez:= .F.
	
	cVar:= MSMM(SQL->QL_ATIVIDA,70)
	For i:=1 to MlCount(cVar,70)
		cLine:= MSMM(SQL->QL_ATIVIDA,70,i)
		cDetalhe:=Space(20)+cLine
		Impr(cDetalhe,"C")
		lAtiv := .F.
	Next i	
	
	If lAtiv
		cDetalhe:= Space(20)+ OemtoAnsi("*** Atividade nao cadastrada ***")
		Impr(cDetalhe,"C")
	EndIf
	
	dbSelectArea("SQL")	
	dbSkip()
EndDo

If l1Vez
	cDetalhe:= Space(15)+ OemtoAnsi("*** Historico Profissional nao cadastrada ***")
	Impr(cDetalhe,"C")
EndIf

cDetalhe:=""
Impr(cDetalhe,"C")

dbSelectArea(cSavAlias)
dbGoto(nSavReg)

RETURN 
	
