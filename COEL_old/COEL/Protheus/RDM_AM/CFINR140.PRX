			    			CARQTMP->RECEBIDOS  := ((CARQTMP->RECEBIDOS) * -1)
			    			CARQTMP->PAGOS      := ((CARQTMP->PAGOS) * -1)
			    			CARQTMP->DISPON := ((CARQTMP->DISPON) * -1)
			   			MsUnLock()
