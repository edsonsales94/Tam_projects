				    If (FJ3->FJ3_IDENT $ "5" .And. Alltrim(FJ3->FJ3_DESCCG) == "-") 
		                //Desabilita a impressão da linha quando for imprimir o traco
						@Li,00 PSAY Replicate("-",200)
						Li++
