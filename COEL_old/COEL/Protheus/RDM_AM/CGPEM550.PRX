aFill( aTotRegs, 0 )

//Cria indice para pesquisar historico de co-participacao/reembolso de plano de saude
DbSelectArea( "RHP" )
DbSetOrder( 1 )

cIndKeyRHP	:= "RHP_FILIAL+RHP_MAT+RHP_COMPPG+RHP_ORIGEM+RHP_CODIGO+RHP_TPLAN+RHP_TPFORN+RHP_CODFOR+RHP_PD" 
