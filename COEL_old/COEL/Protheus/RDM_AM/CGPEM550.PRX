Static Function fCorHistDat()

Local aArea	:= GetArea()

DbSelectArea( "RHS" )
RHS->( dbGoTop() )
While RHS->( !EoF() )
	If Empty( RHS->RHS_DATPGT )
		DbSelectArea( "SRD" )
		If SRD->( dbSeek(RHS->RHS_FILIAL+RHS->RHS_MAT+RHS->RHS_COMPPG+RHS->RHS_PD) )
	       	RHS->( RecLock( "RHS", .F. ) )
	        RHS->RHS_DATPGT := SRD->RD_DATPGT
	        RHS->( MsUnLock() )			
		EndIf
	EndIf
 	RHS->( dbSkip() )				 
End While

DbSelectArea( "RHP" )
RHP->( dbGoTop() )
While RHP->( !EoF() )
	If Empty( RHP->RHP_DATPGT )
		DbSelectArea( "SRD" )
		If SRD->( dbSeek(RHP->RHP_FILIAL+RHP->RHP_MAT+RHP->RHP_COMPPG+RHP->RHP_PD) )
	      	RHP->( RecLock( "RHP", .F. ) )
	       	RHP->RHP_DATPGT := SRD->RD_DATPGT
	      	RHP->( MsUnLock() )			
		EndIf
	EndIf
 	RHP->( dbSkip() )				 
End While

RestArea(aArea)

Return(Nil)
