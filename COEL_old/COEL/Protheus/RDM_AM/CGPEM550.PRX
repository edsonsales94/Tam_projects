If !fValBenNIF()
	Return
EndIf

aRegs := {}

Pergunte("GPM550",.F.)

If lVersao101
	tNewProcess():New( "GPEM550" , cCadastro , bProcesso , STR0002 , "GPM550",,,,,.T.,.T.  ) 	
Else
	AADD(aSays,OemToAnsi(STR0002) ) 									 //"Este programa gera o arquivo da D.I.R.F."
                   
