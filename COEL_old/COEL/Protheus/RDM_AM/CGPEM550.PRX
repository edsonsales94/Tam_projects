	AADD(aButtons, { 1,.T.,{|o| nOpca := 1,IF(gpm550OK(),FechaBatch(),nOpca:=0) }} )
	AADD(aButtons, { 2,.T.,{|o| FechaBatch() }} )
		
	FormBatch( cCadastro, aSays, aButtons )

	IF nOpca == 1
		ProcGpe({|lEnd| GPM550Processa()})  // Chamada do Processamento
	Endif

