User Function INCOME06()
   Local lRet := .T.

	SY1->(DbSetOrder(3))
	If SY1->(DbSeek(xFilial("SY1")+__CUSERID))
		lRet := .T.
	Else
      Alert("Não é permitido informar o comprador !")
		lRet := .F.
	EndIf

Return lRet
